{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "link-name": "off"
      }
    ],
    "compat-api/css": "off",
    "disown-opener": "off",
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off"
      }
    ],
    "no-inline-styles": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 108",
    "not and_ff <= 107",
    "not chrome <= 109",
    "not edge <= 108",
    "not firefox <= 108",
    "not ios_saf <= 16.2",
    "not opera <= 92",
    "not safari <= 16.2",
    "not samsung <= 19"
  ]
}